{
  "version": 2,
  "name": "aboutwater-route-optimizer",
  "framework": null,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "templates/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/server-info",
      "dest": "/main.py"
    },
    {
      "src": "/",
      "dest": "/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "PYTHONPATH": ".",
    "HOST": "0.0.0.0",
    "PORT": "8000"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30,
      "runtime": "python3.9"
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": null,
  "outputDirectory": null
}